
	<div id="pageSignup" style="display:none">
		<div class="login-box">
			<div class="logo">
				<a href="/"><b>Stagify</b></a>
				<small>Gestion de stages d'observation</small>
			</div>
			<div class="row clear-fix">
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<div class="card">
						<div class="header">
							<h2>Créer un compte</h2>
						</div>
						<div class="body">
							<form id="form_advanced_validation" method="POST">

								<div class="form-group form-float">
									<div class="form-line">
										<input class="form-control" name="pseudo" maxlength="30" minlength="1" required aria-required="true" type="text">
										<label class="form-label">Pseudo</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="form-control" name="mdp1" maxlength="60" minlength="1" required aria-required="true" type="password">
										<label class="form-label">Mot de passe</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="form-control" name="mdp2" maxlength="60" minlength="1" required aria-required="true" type="password">
										<label class="form-label">Confirmation mot de passe</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="form-control" name="nom" maxlength="50" minlength="1" required aria-required="true" type="text">
										<label class="form-label">Nom</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="form-control" name="prenom" maxlength="30" minlength="1" required aria-required="true" type="text">
										<label class="form-label">Prénom</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="datepicker form-control" name"dateNaissance" required aria-required="true" type="text">
										<label class="form-label">Date de naissance</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="datepicker form-control" name="tel" required aria-required="true" type="text">
										<label class="form-label">Téléphone</label>
									</div>
								</div>

								<div class="form-group form-float">
									<div class="form-line">
										<input class="datepicker form-control" name="email" required aria-required="true" type="text">
										<label class="form-label">Email</label>
									</div>
								</div>

								<button class="btn btn-primary waves-effect" type="submit">S'inscrire</button>
								<button class="btn btn-primary waves-effect" type="reset">Annuler</button>

							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
