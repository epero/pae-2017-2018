		<body class="login-page">
			<div id="pageLogin" style="display: none">
				<div class="login-page">
					<div class="login-box">
						<div class="logo">
							<a href="/"><b>Stagify</b></a>
							<small>Gestion de stages d'observation</small>
						</div>
						<div class="card">
							<div class="body">
									<div class="msg">Connectez-vous !</div>
									<div id="notification"></div>
									<form id="formLogin" onsubmit="event.preventDefault();" method="post">
										<div class="input-group">
											<span class="input-group-addon">
												<i class="material-icons">person</i>
											</span>
											<div class="form-line">
												<input type="text"  id="inputLogin" class="form-control" name="pseudo" placeholder="Nom d'utilisateur" maxlength="60" required autofocus>
											</div>
										</div>
										<div class="input-group">
											<span class="input-group-addon">
												<i class="material-icons">lock</i>
											</span>
											<div class="form-line">
												<input type="password" id="inputMdp" class="form-control" name="mdp" placeholder="Mot de passe" maxlength="60" required>
											</div>
										</div>
										<div class="row">
											<div class="col-xs-12">
												<button id ="btnLogin" class="btn btn-block btn-lg bg-green waves-effect" type="submit">Se connecter</button>
												<button id ="btnSignUp" class="btn btn-block btn-lg bg-cyan waves-effect" type="button">S'inscrire</button>
											</div>
										</div>
									</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</body>