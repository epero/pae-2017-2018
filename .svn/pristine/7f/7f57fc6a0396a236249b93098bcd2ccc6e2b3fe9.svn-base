function showNavBar(){
    $('nav').show();
}

function hideNavBar(){
    $('nav').hide();
}

$(function() {

    $.when(whoami).then(function() {
        if(whoami.estAdmin === true){
            $('#student-dashboard').show();
        }
    });

    $('#logout').click(function(){
		config = {

					'data': {
						'action': 'logout'
					}
				 }
		util.myajax(config);
        whoami = null;
        hideNavBar();
		FrontEnd.displayPage('pageLogin');
    });
    
    
    $('#personnesContact').click(function(){
    	console.log("$('#personnesContact').click");
		FrontEnd.displayPage('pageEntreprises');
		//FrontEnd.displayPage('pagePersonnesContact');
    });
});