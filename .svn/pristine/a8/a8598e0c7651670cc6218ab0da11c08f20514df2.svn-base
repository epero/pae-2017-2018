<div class="row clearfix datatables-entreprises">
	<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="pageEntreprises" style="display:none;">
		<div class="container-fluid">
			<div class="row clearfix">
				<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
					<div class="card">
						<div class="header">
							<h2 class="titre">
								ENTREPRISES
							</h2>
							<small>
								Sélectionner une entreprise afin de visualiser les personnes de contact associées à celle-ci.
							</small>
						</div>
						<div class="body">
							<div class="table-responsive">
								<table id="dtEntreprises" class="table table-bordered table-striped table-hover dataTable table-condensed" cellspacing="0"
								 width="100%">
									<thead>
										<tr>
											<th>Dénomination</th>
											<!--<th>Ville</th>-->
											<!--  <th>Email</th>-->
											<!--<th>Téléphone</th>-->
											<!--<th>BlackList</th>-->
										</tr>
									</thead>
									<tbody>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>

				<div id="pagePersonnesContact" style="display:none;">
					<div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
						<div class="card">
							<div class="header">
								<h2 class="titre">
									INFOS DE L'ENTREPRISE
								</h2>
								<div class="row clearfix">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
										<div class="demo-google-material-icon">
											<i class="material-icons">call</i>
											<span class="icon-name" id="entrepriseTel"></span>
										</div>
									</div>
									<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4"></div>
									<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
										<div class="demo-google-material-icon">
											<i class="material-icons">email</i>
											<span class="icon-name" id="entrepriseEmail"></span>
										</div>
									</div>
								</div>
							</div>
							<div class="body" style="display:  flex;">
								<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
									<div id="map" class="gmap"></div>
								</div>
								<div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
									<div class="table-responsive">
										<button id="addRow" class="btn btn-lg bg-cyan waves-effect">Ajouter une personne de contact</button>
										<form id="formNewPersonneContact" onsubmit="event.preventDefault();" method="post">
											<button id="btnAddPersonContact" class="btn btn-lg bg-cyan waves-effect" style="display:none" type="submit">Ajouter</button>
											<button id="btnCancelAddPersonContact" style="display:none" class="btn btn-lg bg-grey waves-effect" type="reset">Annuler</button>
											<table id="dtPersonnesContact" class="table table-bordered table-striped table-hover dataTable table-condensed" cellspacing="0"
											 width="100%">
												<thead>
													<tr>
														<th>Nom</th>
														<th>Prénom</th>
														<th>Email</th>
														<th>Téléphone</th>
													</tr>
												</thead>
												<tbody>

												</tbody>
											</table>
										</form>
										<table id="newRow" style="display:none">
											<tbody>
												<tr id="-1">
													<td>
														<div class="form-group form-float">
															<div class="form-line">
																<input type="text" class="form-control" name="nom" maxlength="50" minlength="1" required aria-required="true">
															</div>
														</div>
													</td>
													<td>
														<div class="form-group form-float">
															<div class="form-line">
																<input type="text" class="form-control" name="prenom" maxlength="30" minlength="1" required aria-required="true">
															</div>
														</div>
													</td>
													<td>
														<div class="form-group form-float">
															<div class="form-line">
																<input type="text" class="form-control" name="email" maxlength="50" minlength="1" required aria-required="true">
															</div>
														</div>
													</td>
													<td>
														<div class="form-group form-float">
															<div class="form-line">
																<input type="text" class="form-control" name="tel" maxlength="15" minlength="1" required aria-required="true">
															</div>
														</div>
													</td>
												</tr>
											</tbody>
										</table>
										<!-- Initier un contact -->
										<div id="confirmInitiateContact" style="display:none;">
											<p id="txtInitiateContact" class="font-bold text-center"></p>
											<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
												<button id="btnAddContactConfirm" class="btn btn-block btn-lg btn-warning waves-effect" type="button">Confirmer</button>
											</div>
											<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
												<button id="btnCancelAddContact" class="btn btn-block btn-lg bg-grey waves-effect" type="button">Annuler</button>
											</div>
										</div>
										<div id="btnsContactEntreprises">
											<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
												<button id="btnAddContact" class="btn btn-block btn-lg bg-blue-grey waves-effect" type="button">Initier un contact</button>
											</div>
										</div>
									</div>
								</div>
							</div>
