import util.AppContext.DependanceInjection;
import util.Util;

public class StageUccImpl implements StageUcc {

  @DependanceInjection
  private StageDao stageDao;

  @DependanceInjection
  private DalServices dalServices;

  @Override
  public StageDto confirmDataStage(ContactDto contact, PersonneContactDto personneContact) {
    Util.checkObject(contact);
    Util.checkObject(personneContact);
    // TODO check biz

    StageDto stageEnDb = null;
    try {
      dalServices.startTransaction();
      // TODO autres verifs biz
      // stageEnDb = stageDao.insertStage;
    } catch (Exception ex) {
      dalServices.rollback();
      throw ex;
    } finally {
      dalServices.commitTransaction();
    }
    return stageEnDb;
  }



}
